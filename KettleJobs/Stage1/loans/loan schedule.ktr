  <step>
    <name>loan schedule input</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>SourceDB</connection>
    <sql>SELECT 
  ls.ID As 	id
, ls.ACCOUNT_ID As 	loan_id
, if(ls.installment_id = 1 , la.disbursement_date , lsp.action_date ) As fromDate
, ls.ACTION_DATE As 	duedate
, ls.INSTALLMENT_ID As 	installment
, if(ls.INTEREST &lt; 0, ls.PRINCIPAL + ls.INTEREST, ls.PRINCIPAL) As 	principal_amount
, if(ac.account_state_id = 7 AND ls.PAYMENT_DATE = ac.closed_date , 0 , if(ac.account_state_id = 8 AND ls.PAYMENT_DATE = ac.UPDATED_DATE , 0 , ls.PRINCIPAL_PAID ) ) As 	principal_completed_derived
, if(ac.account_state_id = 7 AND ls.PAYMENT_DATE = ac.closed_date , ls.PRINCIPAL_PAID , 0 ) As 	principal_writtenoff_derived
, if(ls.INTEREST &lt; 0, 0, ls.INTEREST) As 	interest_amount
, if(ac.account_state_id = 7 AND ls.PAYMENT_DATE = ac.closed_date , 0 , if(ac.account_state_id = 8 AND ls.PAYMENT_DATE = ac.UPDATED_DATE , 0 , ls.INTEREST_PAID ) ) As 	interest_completed_derived
, if(ac.account_state_id = 7 AND ls.PAYMENT_DATE = ac.closed_date , ls.INTEREST_PAID , 0 ) As 	interest_writtenoff_derived
, 0 As interest_waived_derived -- for SECDEP no interest waive of for early payment
-- if(ac.account_state_id = 6 AND ls.ACTION_DATE &gt; ac.closed_date , ls.INTEREST_PAID , 0 ) As 	interest_waived_derived
, ls.MISC_FEES As 	fee_charges_amount
, ls.MISC_FEES_PAID As 	fee_charges_completed_derived
, NULL As 	fee_charges_writtenoff_derived
, NULL As 	fee_charges_waived_derived
, NULL As 	penalty_charges_amount
, NULL As 	penalty_charges_completed_derived
, NULL As 	penalty_charges_writtenoff_derived
, NULL As 	penalty_charges_waived_derived
, NULL As 	total_paid_in_advance_derived
, NULL As 	total_paid_late_derived
, ls.PAYMENT_STATUS As 	completed_derived
, IF(( ac.account_state_id = 6 OR ac.account_state_id = 7 OR ac.account_state_id = 8 )  AND ls.PAYMENT_DATE &gt;= ac.CLOSED_DATE , IF ( ac.CLOSED_DATE IS NULL , ac.UPDATED_DATE, ac.CLOSED_DATE ) , IF (ls.PAYMENT_STATUS = 1 , ls.PAYMENT_DATE , null)  )   As 	obligations_met_on_date
, ac.CREATED_BY As 	createdby_id
, ac.CREATED_DATE As 	created_date
, ac.CREATED_DATE As 	lastmodified_date
, ac.CREATED_BY As 	lastmodifiedby_id
, la.disbursement_date As DisbDate
FROM loan_schedule ls
LEFT JOIN loan_schedule lsp ON lsp.account_id = ls.account_id AND ls.installment_id &gt; 1 AND lsp.installment_id  =  (ls.installment_id - 1)
JOIN loan_account la ON la.ACCOUNT_ID = ls.ACCOUNT_ID
JOIN account ac ON ac.ACCOUNT_ID = la.ACCOUNT_ID 
JOIN customer cr ON cr.CUSTOMER_ID = ac.CUSTOMER_ID
JOIN meeting me ON me.MEETING_ID = la.MEETING_ID
JOIN recurrence_detail rd ON rd.MEETING_ID = me.MEETING_ID</sql>
